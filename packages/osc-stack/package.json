{
  "name": "osc-academic-hub",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "prepare": "husky install",
    "build": "npm-run-all -p build:*",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "build:css": "PORT=3001 LOAD_PATH=app/styles PATH_TO_MAIN_SCSS=/app/styles/main.scss PATH_TO_DEST_MAIN_CSS=/app/styles/dest/main.css PATH_TO_COMPONENTS=/app/components node bin/postcssBuild.js",
    "dev:css": "PORT=3001 LOAD_PATH=app/styles PATH_TO_MAIN_SCSS=/app/styles/main.scss PATH_TO_DEST_MAIN_CSS=/app/styles/dest/main.css node bin/postcss.js",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "dev": "npm-run-all -p dev:*",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --max-warnings 0 --no-error-on-unmatched-pattern .",
    "lint-and-fix": "eslint --cache --cache-location ./node_modules/.cache/eslint --max-warnings 0 --ext .ts,.tsx, .js, .jsx --fix --no-error-on-unmatched-pattern .",
    "prettier-format": "prettier --config .prettierrc.json '*.{js,jsx,ts,tsx,json,scss}' '**/*.{js,jsx,ts,tsx,json,scss}' --write",
    "stylelint": "npx stylelint \"**/*.scss\" --allow-empty-input",
    "stylelint-and-fix": "npx stylelint \"**/*.scss\" --allow-empty-input --fix",
    "setup": "prisma migrate dev && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./mocks -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "build-storybook": "cross-env NODE_ENV=production npm run build:css && build-storybook",
    "storybook": "npm run build:css && concurrently \"npm run watch:css\" \"start-storybook -p 6006\"",
    "chromatic": "npx chromatic --exit-zero-on-changes"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@chakra-ui/icons": "^2.0.2",
    "@chakra-ui/react": "^2.1.2",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@prisma/client": "^3.14.0",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@remix-run/server-runtime": "^1.5.1",
    "@storybook/addon-a11y": "^6.5.9",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.3.10",
    "isbot": "^3.5.0",
    "isomorphic-dompurify": "^0.19.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.2.0",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^4.0.1",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@faker-js/faker": "^6.3.1",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@remix-run/dev": "^1.5.1",
    "@remix-run/eslint-config": "^1.5.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.11",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.35",
    "@types/node-persist": "^3.1.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "chromatic": "^6.5.6",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "cypress": "^9.6.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^3.2.2",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "msw": "^0.40.2",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prisma": "^3.14.0",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.12.8"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "browserslist": [
    "last 2 versions, not dead"
  ],
  "lint-staged": {
    "*.scss": [
      "npm run stylelint-and-fix"
    ],
    "*.(js|ts|tsx|jsx|json|scss)": [
      "npm run prettier-format"
    ],
    "*.(js|ts|tsx/jsx)": [
      "npm run lint-and-fix"
    ]
  }
}
