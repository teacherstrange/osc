{
  "name": "osc-academic-hub",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "scripts": {
    "setup": "prisma db seed",
    "seed": "cross-env DATABASE_URL=file:./data.db?connection_limit=1 ts-node --require tsconfig-paths/register prisma/seed.ts",
    "build": "run-s build:*",
    "build:css": "PORT=3001 LOAD_PATH=app/styles PATH_TO_MAIN_SCSS=/app/styles/main.scss PATH_TO_DEST_MAIN_CSS=/app/styles/dest/main.css PATH_TO_COMPONENTS=/app/components node bin/postcssBuild.js",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development npm run build:css && run-p dev:*",
    "dev:css": "PORT=3001 LOAD_PATH=app/styles PATH_TO_MAIN_SCSS=/app/styles/main.scss PATH_TO_DEST_MAIN_CSS=/app/styles/dest/main.css node bin/postcss.js",
    "dev:remix": "cross-env PORT=3000 NODE_ENV=development binode --require ./__mocks__ -- @remix-run/dev:remix dev",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./__mocks__ -- @remix-run/serve:remix-serve build",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "cross-env PORT=8812 start-server-and-test start:mocks http://localhost:8812 \"PORT=8811 npx playwright test\"",
    "typecheck": "tsc -b",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-icons": "^1.2.0",
    "@remix-run/node": "^1.13.0",
    "@remix-run/react": "^1.13.0",
    "@remix-run/serve": "^1.13.0",
    "@remix-run/server-runtime": "^1.13.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "node-persist": "^3.1.3",
    "osc-ui": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.5.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@playwright/test": "^1.31.1",
    "@remix-run/dev": "^1.13.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^18.14.2",
    "@types/node-persist": "^3.1.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "binode": "^1.0.5",
    "c8": "^7.13.0",
    "cross-env": "^7.0.3",
    "msw": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  },
  "prisma": {
    "seed": "lerna run seed"
  },
  "browserslist": [
    "last 2 versions, not dead"
  ]
}
