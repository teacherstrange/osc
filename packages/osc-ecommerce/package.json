{
  "name": "osc-ecommerce",
  "private": true,
  "version": "0.0.0",
  "sideEffects": false,
  "scripts": {
    "setup": "prisma db seed",
    "seed": "cross-env DATABASE_URL=file:./data.db?connection_limit=1 ts-node --require tsconfig-paths/register prisma/seed.ts",
    "build": "run-s build:*",
    "build:css": "sass app/:app --no-source-map",
    "build:postcss": "postcss app/{components,styles}/**/*.css -r",
    "build:remix": "cross-env NODE_ENV=production remix build && npm run copy-fonts:from-remix",
    "build:server": "tsc --project tsconfig.server.json",
    "dev": "npm-run-all build --parallel \"dev:*\"",
    "dev:css": "sass --watch app/:app/ --stop-on-error",
    "dev:node": "cross-env PORT=2000 NODE_ENV=development nodemon --require dotenv/config ./server.ts --watch ./server.ts --ext js,jsx,ts,tsx",
    "dev:remix": "remix watch",
    "start": "cross-env NODE_ENV=production node --require dotenv/config ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require dotenv/config --require ./__mocks__ ./build/server.js",
    "test": "vitest run --coverage",
    "test:watch": "vitest --coverage",
    "pretest:e2e": "npm run build",
    "test:e2e": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx playwright test\"",
    "test:e2e:ui": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx playwright test --ui\"",
    "typecheck": "tsc -b",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch",
    "copy-fonts:from-ui": "cp -R ../osc-ui/src/fonts public",
    "copy-fonts:from-remix": "cp -R public/fonts public/build/_assets",
    "copy-fonts": "run-s copy-fonts:*"
  },
  "dependencies": {
    "@hubspot/api-client": "^8.8.0",
    "@prisma/client": "^4.11.0",
    "@radix-ui/react-icons": "^1.2.0",
    "@radix-ui/react-slot": "^1.0.1",
    "@react-aria/i18n": "^3.7.0",
    "@react-aria/ssr": "^3.5.0",
    "@remix-run/express": "^1.12.0",
    "@remix-run/node": "^1.12.0",
    "@remix-run/react": "^1.12.0",
    "@remix-run/server-runtime": "^1.12.0",
    "@sanity/preview-kit": "^1.3.8",
    "@shopify/hydrogen": "^2023.1.5",
    "algoliasearch": "^4.17.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-tag": "^2.12.6",
    "groq": "^3.5.1",
    "morgan": "^1.10.0",
    "node-persist": "^3.1.3",
    "osc-design-tokens": "*",
    "osc-ui": "*",
    "picosanity": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-instantsearch-hooks": "^6.44.0",
    "react-instantsearch-hooks-server": "^6.44.0",
    "react-instantsearch-hooks-web": "^6.44.0",
    "remix-utils": "^6.0.0",
    "tiny-invariant": "^1.3.1",
    "web-push": "^3.5.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@playwright/test": "^1.33.0",
    "@remix-run/dev": "^1.12.0",
    "@remix-run/node": "^1.0.3",
    "@sanity/groq-store": "^2.0.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.14.2",
    "@types/node-persist": "^3.1.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitest/coverage-c8": "^0.29.2",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "c8": "^7.13.0",
    "cssnano": "^5.1.15",
    "msw": "^1.1.0",
    "nodemon": "^2.0.21",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.33.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "react-router-dom": "^6.8.2",
    "sass": "^1.58.3",
    "start-server-and-test": "^2.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.29.2"
  },
  "prisma": {
    "seed": "lerna run seed"
  },
  "browserslist": [
    "last 2 versions, not dead"
  ],
  "engines": {
    "node": ">=16"
  }
}
