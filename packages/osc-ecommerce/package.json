{
  "name": "osc-ecommerce",
  "private": true,
  "sideEffects": false,
  "version": "0.0.0",
  "scripts": {
    "setup": "prisma db seed",
    "seed": "cross-env DATABASE_URL=file:./data.db?connection_limit=1 ts-node --require tsconfig-paths/register prisma/seed.ts",
    "build": "run-s build:*",
    "build:css": "PORT=2001 LOAD_PATH=app/styles PATH_TO_MAIN_SCSS=/app/styles/main.scss PATH_TO_DEST_MAIN_CSS=/app/styles/dest/main.css PATH_TO_COMPONENTS=/app/components node bin/postcssBuild.js",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "dev": "cross-env NODE_ENV=development npm run build:css && run-p dev:*",
    "dev:css": "PORT=2001 LOAD_PATH=app/styles PATH_TO_MAIN_SCSS=/app/styles/main.scss PATH_TO_DEST_MAIN_CSS=/app/styles/dest/main.css node bin/postcss.js",
    "dev:remix": "cross-env PORT=2000 NODE_ENV=development binode --require ./__mocks__ -- @remix-run/dev:remix dev",
    "start": "remix-serve build",
    "start:mocks": "binode --require ./__mocks__ -- @remix-run/serve:remix-serve build",
    "test": "vitest --coverage",
    "test:dev": "vitest run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:2000 \"PORT=2000 npx playwright test\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"PORT=8811 npx playwright test\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "build-storybook": "npm run build:css && lerna run build:css && build-storybook",
    "storybook": "npm run build:css && concurrently \"npm run dev:css\" \"start-storybook -p 6006\"",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "@radix-ui/react-icons": "^1.1.1",
    "@remix-run/node": "^1.5.1",
    "@remix-run/react": "^1.5.1",
    "@remix-run/serve": "^1.5.1",
    "@remix-run/server-runtime": "^1.5.1",
    "@sanity/groq-store": "^0.4.1",
    "@storybook/addon-a11y": "^6.5.9",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "framer-motion": "^6.5.1",
    "groq": "^2.33.2",
    "isomorphic-dompurify": "^0.19.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "osc-ui": "file:packages/osc-ui",
    "picosanity": "^4.0.0",
    "react": "^18",
    "react-dom": "^18",
    "tiny-invariant": "^1.2.0",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@faker-js/faker": "^6.3.1",
    "@fullhuman/postcss-purgecss": "^4.1.3",
    "@nrwl/nx-cloud": "^14.2.0",
    "@playwright/test": "^1.27.1",
    "@portabletext/types": "^1.0.3",
    "@remix-run/dev": "^1.6.5",
    "@remix-run/eslint-config": "^1.5.1",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.2",
    "@types/node": "^17.0.35",
    "@types/node-persist": "^3.1.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "binode": "^1.0.5",
    "c8": "^7.11.3",
    "chromatic": "^6.7.0",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.12",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "happy-dom": "^3.2.2",
    "husky": "^8.0.1",
    "lerna": "^5.0.0",
    "lint-staged": "^13.0.1",
    "msw": "^0.47.3",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "nx": "^14.3.6",
    "postcss": "^8.4.14",
    "prettier": "2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "sass": "^1.53.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.18.1"
  },
  "prisma": {
    "seed": "lerna run seed"
  },
  "browserslist": [
    "last 2 versions, not dead"
  ]
}
