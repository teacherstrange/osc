{
  "name": "osc-ui",
  "version": "0.0.0",
  "main": "dist/index.js",
  "files": [
    "dist/*.css"
  ],
  "scripts": {
    "build": "npm run build:css && rm -rf dist && rollup --config",
    "build:css": "PORT=3001 LOAD_PATH=src/styles PATH_TO_MAIN_SCSS=/src/styles/main.scss PATH_TO_DEST_MAIN_CSS=/src/styles/dest/main.css PATH_TO_COMPONENTS=/src/components node bin/postcssBuild.js",
    "test": "jest",
    "dev": "npm run build && run-p dev:*",
    "dev:rollup": "rollup -c -w",
    "dev:css": "PORT=3001 LOAD_PATH=src/styles PATH_TO_MAIN_SCSS=/src/styles/main.scss PATH_TO_DEST_MAIN_CSS=/src/styles/dest/main.css node bin/postcss.js"
  },
  "dependencies": {
    "rollup-plugin-postcss": "^4.0.2",
    "@storybook/addon-a11y": "^6.5.9",
    "@testing-library/react": "13.3.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^17.0.14",
    "jest": "^27.4.3",
    "rollup": "^2.60.2",
    "rollup-plugin-typescript2": "^0.32",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.2"
  },
  "peerDependencies": {
    "@chakra-ui/icons": "*",
    "@chakra-ui/react": "*",
    "@chakra-ui/system": "*",
    "framer-motion": "*",
    "react": "*",
    "react-dom": "*"
  }
}
