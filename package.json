{
  "name": "osc",
  "version": "0.51.0",
  "private": true,
  "workspaces": [
    "packages/**"
  ],
  "scripts": {
    "prepare": "husky install",
    "setup": "npm i && npx prisma generate",
    "clean": "npm exec --workspaces -- npx rimraf -v node_modules && npx rimraf -v node_modules package-lock.json",
    "stylelint": "npx stylelint \"**/*.scss\" --allow-empty-input --formatter verbose",
    "stylelint-and-fix": "npx stylelint \"**/*.scss\" --allow-empty-input --fix ",
    "lint": "DEBUG=eslint:cli-engine eslint --cache --cache-location ./node_modules/.cache/eslint  -c ./.eslintrc.js --max-warnings 0 --ext .ts,.tsx,.js,.jsx --no-error-on-unmatched-pattern .",
    "lint-and-fix": "eslint -c ./.eslintrc.js --cache --cache-location ./node_modules/.cache/eslint  --max-warnings 0 --ext .ts,.tsx,.js,.jsx --fix --no-error-on-unmatched-pattern",
    "prettier-format": "prettier --config .prettierrc.json '*.{js,jsx,ts,tsx,json,scss}' '**/*.{js,jsx,ts,tsx,json,scss}' --write",
    "typecheck": "npx nx run-many --target=typecheck --all",
    "build-storybook": "cross-env NODE_ENV=production npx nx run osc-ui:build && build-storybook",
    "storybook": "concurrently \"npm run dev:osc-ui --parallel\" \"start-storybook --quiet -p 6006\"",
    "chromatic": "chromatic --exit-zero-on-changes",
    "build": "npx nx run-many --target=build --all",
    "build:osc-ui": "npx nx run osc-ui:build",
    "build:osc-studio": "npx nx run osc-studio:build",
    "build:osc-ecommerce": "npx nx run osc-ecommerce:build",
    "build:osc-academic-hub": "npx nx run osc-academic-hub:build",
    "test": "npx nx run-many --target=test --all",
    "test:osc-ui": "npx nx run osc-ui:test",
    "test:osc-ecommerce": "npx nx run osc-ecommerce:test",
    "test:osc-academic-hub": "npx nx run osc-academic-hub:test",
    "test:watch:osc-ui": "npx nx run osc-ui:test:watch",
    "test:watch:osc-ecommerce": "npx nx run osc-ecommerce:test:watch",
    "test:watch:osc-academic-hub": "npx nx run osc-academic-hub:test:watch",
    "test:e2e": "npx nx run-many --target=test:e2e --all",
    "test:e2e:osc-ecommerce": "npx nx run osc-ecommerce:test:e2e",
    "test:e2e:osc-academic-hub": "npx nx run osc-academic-hub:test:e2e",
    "dev": "npx nx run-many --target=dev --all",
    "dev:fe": "npx nx run-many --target=dev --projects=osc-ui,osc-studio,osc-ecommerce,osc-academic-hub",
    "dev:osc-ui": "npx nx run osc-ui:dev",
    "dev:osc-ecommerce": "npx nx run-many --target=dev --projects=osc-ui,osc-ecommerce",
    "dev:osc-academic-hub": "npx nx run-many --target=dev --projects=osc-ui,osc-academic-hub",
    "dev:osc-studio": "npx nx run osc-studio:dev",
    "dev:api": "npx nx run-many --target=dev --projects=osc-api-*",
    "api:dev:osc-api-auth": "npx nx run osc-api-auth:dev",
    "api:dev:osc-api-ecommerce": "npx nx run osc-api-ecommerce:dev",
    "api:dev:osc-api-crm": "npx nx run osc-api-crm:dev",
    "api:dev:osc-api-admin": "npx nx run osc-api-admin:dev",
    "api:dev:osc-api-lms": "npx nx run osc-api-lms:dev",
    "api:dev:osc-api-assignments": "npx nx run osc-api-assignments:dev",
    "api:dev:osc-api-gateway": "npx nx run osc-api-gateway:dev",
    "tokens:generate": "npx nx run osc-design-tokens:generate",
    "copy-fonts:from-ui": "npx nx run-many --target=copy-fonts:from-ui --projects=osc-academic-hub,osc-ecommerce",
    "copy-fonts:from-remix": "npx nx run-many --target=copy-fonts:from-remix --projects=osc-academic-hub,osc-ecommerce",
    "copy-fonts": "npx nx run-many --target=copy-fonts --projects=osc-academic-hub,osc-ecommerce"
  },
  "lint-staged": {
    "*.scss": [
      "npm run stylelint-and-fix"
    ],
    "*.{js|ts|tsx|jsx|json|scss}": [
      "npm run prettier-format"
    ],
    "*.{ts|tsx|jsx}": [
      "npm run lint-and-fix"
    ]
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "15.7.0",
    "@remix-run/eslint-config": "^1.13.0",
    "@storybook/addon-a11y": "^6.5.16",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^6.5.16",
    "@storybook/addon-interactions": "^6.5.16",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/builder-webpack5": "^6.5.16",
    "@storybook/manager-webpack5": "^6.5.16",
    "@storybook/react": "^6.5.16",
    "@storybook/testing-library": "^0.0.13",
    "@types/eslint": "^8.21.1",
    "chromatic": "^6.17.0",
    "concurrently": "^7.6.0",
    "conventional-changelog-conventionalcommits": "^5.0.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "npm-run-all": "^4.1.5",
    "nx": "15.7.0",
    "prettier": "^2.6.2",
    "prisma": "^4.11.0",
    "storybook-addon-sass-postcss": "^0.1.3",
    "stylelint": "^14.8.2",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "~4.8.2"
  },
  "overrides": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "node-sass": "^8"
  }
}
